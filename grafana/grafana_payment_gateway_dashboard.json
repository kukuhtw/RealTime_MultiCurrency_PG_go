      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "(sum(increase(payment_requests_total{status=\"FAILED\"}[5m])) / sum(increase(payment_requests_total[5m]))) * 100",
          "legendFormat": "Error % (5m)",
          "refId": "A"
